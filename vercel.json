{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api/|_next/|static/|assets/|favicon.ico|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}
